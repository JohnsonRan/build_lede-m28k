-+	rt2x00dev->eeprom_file = &mtd_fw;
-+	mtd_fw.data = (const u8 *) rt2x00dev->eeprom;
-+
-+	dev_info(rt2x00dev->dev, "loaded eeprom from mtd device \"%s\"\n", part);
-+#endif
-+
-+	return ret;
-+}
-+#endif
